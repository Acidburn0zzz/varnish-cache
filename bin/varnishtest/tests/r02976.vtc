varnishtest "Detect probe interval inversion"

server s1 -repeat 5 { # probe requests
	rxreq
	txresp
} -start

varnish v1 -cliok {param.set vcc_err_unref off}

varnish v1 -vcl {
	backend b1 {
		.host = "${s1_sock}";
		.probe = { .interval = 1s; }
	}

	backend b2 {
		.host = "${bad_backend}";
		.probe = { .interval = 24h; }
	}
} -start

server s1 -wait
