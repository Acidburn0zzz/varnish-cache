varnishtest	"trivial run of varnishhist in curses mode"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {} -start

process p1 -dump {varnishhist -n ${v1_name}} -start

process p1 -need-bytes 1

client c1 {
	txreq
	rxresp
} -run

process p1 -need-bytes 200 -screen_dump -write {q} -wait
